package com.java;

/*
*@author: Mahabir Prasad Mohapatra
*/
public class Student {
	String name;
	int rollno;
	int age;

	public Student(String name, int rollno, int age) {
		super();
		this.name = name;
		this.rollno = rollno;
		this.age = age;
	}

	@Override
	public String toString() {
		return "Student [name=" + name + ", rollno=" + rollno + ", age=" + age + "]";
	}

}
