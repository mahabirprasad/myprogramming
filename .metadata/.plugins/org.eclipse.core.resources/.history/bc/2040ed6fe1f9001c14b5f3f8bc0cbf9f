package com.java;

public class A {

	public static void main(String[] args) {
		
			    String s = "MAHA$,BIR";
			    String str="";
			    char[] ch = s.toCharArray();
			    for (int i =0;i<=ch.length/2 ;i++ ){
			        char t = ch[i];
			        ch[i]=ch[ch.length-1-i];
			        ch[ch.length-1-i]= t;
			    } 
			    str=new String(ch);
			    System.out.println(str);
			
	}

}
